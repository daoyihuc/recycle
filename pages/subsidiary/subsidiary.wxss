.subsidiary .tabbar  {
    display: flex;
    margin-top: 45rpx;
}

.subsidiary .tabbar>view  {
    -webkit-box-flex: 1;
    flex: 1;
    display: flex;
    -webkit-box-align: center;
    align-items: center;
    -webkit-box-pack: center;
    justify-content: center;
    padding: 10rpx 0;
}

.subsidiary .tabbar>view image  {
    width: 34rpx;
    height: 34rpx;
}

.subsidiary .tabbar>view view  {
    font-size: 32rpx;
    margin-left: 15rpx;
}

.subsidiary .border  {
    border-right: 2rpx solid #ccc;
}

.subsidiary .segmentation  {
    width: 744rpx;
    height: 28rpx;
    margin: 0 auto;
    margin-top: 45rpx;
}

.subsidiary .segmentation image  {
    width: 100%;
    height: 100%;
}

.subsidiary .noData  {
    margin-top: 50rpx;
}

.subsidiary .noData image  {
    width: 100%;
}

.subsidiary .orange  {
    color: #fdca30;
}

.con  {
    width: 744rpx;
    margin: 15rpx auto;
}

.con .item  {
    display: flex;
    -webkit-box-pack: justify;
    justify-content: space-between;
    -webkit-box-align: center;
    align-items: center;
    padding: 30rpx;
    box-sizing: border-box;
}

.con .item>view  {
    max-width: 650rpx;
}

.con .item>view text  {
    text-overflow: ellipsis;
    overflow: hidden;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
}

.con .item>view .description  {
    text-overflow: ellipsis;
    overflow: hidden;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    word-wrap: break-word;
    font-size: 26rpx;
}