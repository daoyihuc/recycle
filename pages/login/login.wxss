.login {
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
    align-content: center;
    align-items: center;
    justify-content: center;
    margin-top: 100rpx;
}
.labelText{

    font-size: 48rpx;
    font-family: PingFang SC;
    font-weight: 400;
    color: #000000;
    line-height: 43rpx;
}
.labelImage{
    width: 515rpx;
    height: 397rpx;
    margin-top: 119rpx;
}

.btn  {
    display: flex;
    flex-direction: column;
    /*justify-content: center;*/
    align-items: center;
}

.btn-1{
    width: 610rpx;
    height: 90rpx;
    background: #01BC6B;
    border-radius: 45px;
    color: white;
    margin-top: 100rpx;
}

.btn-2 {
    background-color: #fff;
    /*color: silver;*/
    margin-top: 50rpx;

    font-size: 36rpx;
    font-family: PingFang SC;
    font-weight: 400;
    color: #666666;
}

.btn-2::after {
    border: none;
}
