

.itemBox{
    padding: 10rpx 0;
    margin: 0 20rpx;
    border-bottom: 1rpx solid #eeeeee;
}
.button_box{
    width: 100%;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    margin-top: 50rpx;
}
.addItem{
    width: 690rpx;
    height: 96rpx;
    background: white;
    border: 1px solid #01BC6B;
    border-radius: 48rpx;
    font-size: 36rpx;
    font-family: PingFang SC;
    font-weight: bold;
    color: #01BC6B;
}
.payItem{
    width: 690rpx;
    height: 96rpx;
    background: #01BC6B;
    border-radius: 48rpx;

    font-size: 36rpx;
    font-family: PingFang SC;
    font-weight: bold;
    color: #FFFFFF;
    margin-top: 30rpx;

    /*line-height: 37px;*/
}
.margin{
    margin-top: 10rpx;
