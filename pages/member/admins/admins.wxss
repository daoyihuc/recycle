    /*background: #999999;*/
}
.itemMax{
    margin: 10rpx 15rpx;
    background: white;
    box-shadow: 1px 1px 10px 1px #999999;
    border-radius: 10rpx;
    margin-bottom: 100rpx;

}
.itemAddresbox{
    display: flex;
    justify-content: space-between;
    align-items: center;
}
.bottonSize{
    width: 148rpx;
    height: 65rpx;
    background: #fcba30;
    color: white;
    line-height: 65rpx;
    font-size: 30rpx;
}
.address{
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    -webkit-box-align: center;
    align-items: center;
    width: 80%;
}
.img{
    margin-right: 10rpx;
    margin-left: 20rpx;
}
.item{
    padding: 10rpx 0;
}
.itemBox{
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 0 10rpx;
    margin-bottom: 20rpx;

}
.buttonBox{
    display: flex;
    justify-content: space-around;
    padding: 20rpx 0;
}
.phone_box{
    display: flex;
    align-items: center;
}
.phone_icon{
    width: 50rpx;
    height: 50rpx;
}
