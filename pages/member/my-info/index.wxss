.my-info{}
.header {
    display: flex;
    -webkit-box-pack: center;
    justify-content: center;
    -webkit-box-align: center;
    align-items: center;
    border-bottom: 20rpx solid #f5f5f5;
    padding: 60rpx 0;
}

.my-info .header image {
    width: 120rpx;
    height: 120rpx;
}

.my-info .content {
    padding: 0 20rpx;
}
.confirm {
    width: 100%;
    height: 100rpx;
    line-height: 100rpx;
    text-align: center;
    background: #fcba03;
    font-size: 32rpx;
    color: #fff;
    position: absolute;
    bottom: 0;
}
