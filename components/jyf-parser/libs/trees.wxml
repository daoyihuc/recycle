<view class="interlayer" style="line-height:0;">
    <block wx:for="{{ns}}" wx:for-item="n" wx:key="index">
        <rich-text class="_svg" nodes="{{[n]}}" wx:if="{{n.err||n.svg||n.en}}"></rich-text>
        <block wx:else>
            <image binderror="__e" bindload="__e" bindtap="__e" class="_img" data-attrs="{{n.attrs}}" data-auto="{{n.auto}}" data-event-opts="{{[ [ 'tap',[ [ 'imgtap',['$event'] ] ] ],[ 'load',[ [ 'loadImg',['$event'] ] ] ],[ 'error',[ [ 'error',['$event'] ] ] ] ]}}" data-i="{{index}}" data-source="img" lazyLoad="{{lazyLoad}}" mode="{{n.mode||'widthFix'}}" showMenuByLongpress="{{!n.attrs.ignore}}" src="{{n.attrs.src}}" style="{{n.attrs.style}}" webp="{{n.webp}}" wx:if="{{n.name=='img'}}"></image>
            <block wx:else>
                <text decode="{{true}}" wx:if="{{n.type=='text'}}">{{n.text}}</text>
                <block wx:else>
                    <text wx:if="{{n.name=='br'}}">
</text>
                    <block wx:else>
                        <block wx:if="{{n.name=='video'}}">
                            <view bindtap="__e" class="{{[ '_video '+(n.attrs.class||'') ]}}" data-event-opts="{{[ [ 'tap',[ [ '_loadVideo',['$event'] ] ] ] ]}}" data-i="{{index}}" id="{{n.attrs.id}}" style="{{n.attrs.style}}" wx:if="{{!loadVideo||n.lazyLoad}}"></view>
                            <video autoplay="{{n.attrs.autoplay}}" binderror="__e" bindplay="__e" class="{{[n.attrs.class]}}" controls="{{n.attrs.controls}}" data-event-opts="{{[ [ 'error',[ [ 'error',['$event'] ] ] ],[ 'play',[ [ 'play',['$event'] ] ] ] ]}}" data-source="video" id="{{n.attrs.id}}" loop="{{n.attrs.loop}}" muted="{{n.attrs.muted}}" poster="{{n.attrs.poster}}" src="{{n.attrs.source[n.i||0]}}" style="{{n.attrs.style}}" unitId="{{n.attrs['unit-id']}}" wx:else></video>
                        </block>
                        <block wx:else>
                            <audio author="{{n.attrs.author}}" autoplay="{{n.attrs.autoplay}}" binderror="__e" bindplay="__e" class="{{[n.attrs.class]}}" controls="{{n.attrs.controls}}" data-event-opts="{{[ [ 'error',[ [ 'error',['$event'] ] ] ],[ 'play',[ [ 'play',['$event'] ] ] ] ]}}" data-i="{{index}}" data-source="audio" loop="{{n.attrs.loop}}" name="{{n.attrs.name}}" poster="{{n.attrs.poster}}" src="{{n.attrs.source[n.i||0]}}" style="{{n.attrs.style}}" wx:if="{{n.name=='audio'}}"></audio>
                            <block wx:else>
                                <view bindtap="__e" class="{{[ '_a '+(n.attrs.class||'') ]}}" data-attrs="{{n.attrs}}" data-event-opts="{{[ [ 'tap',[ [ 'linkpress',['$event'] ] ] ] ]}}" hoverClass="_hover" style="{{n.attrs.style}}" wx:if="{{n.name=='a'}}">
                                    <trees bind:__l="__l" class="_span" nodes="{{n.children}}" vueId="{{'676b0423-1-'+index}}"></trees>
                                </view>
                                <block wx:else>
                                    <view class="{{[n.attrs.class]}}" id="{{n.attrs.id}}" style="{{(n.attrs.style||'')+';display:flex'}}" wx:if="{{n.name=='li'}}">
                                        <view class="_ol-bef" wx:if="{{n.type=='ol'}}">{{n.num}}</view>
                                        <view class="_ul-bef" wx:else>
                                            <view class="_ul-p1" wx:if="{{n.floor%3==0}}">█</view>
                                            <block wx:else>
                                                <view class="_ul-p2" wx:if="{{n.floor%3==2}}"></view>
                                                <view class="_ul-p1" style="border-radius:50%;" wx:else>█</view>
                                            </block>
                                        </view>
                                        <trees bind:__l="__l" class="_li" lazyLoad="{{lazyLoad}}" nodes="{{n.children}}" vueId="{{'676b0423-2-'+index}}"></trees>
                                    </view>
                                    <block wx:else>
                                        <view class="{{[n.attrs.class]}}" id="{{n.attrs.id}}" style="{{(n.attrs.style||'')+';display:table'}}" wx:if="{{n.name=='table'&&n.c}}">
                                            <view class="{{[tbody.attrs.class]}}" style="{{(tbody.attrs.style||'')+(tbody.name[0]=='t'?';display:table-'+(tbody.name=='tr'?'row':'row-group'):'')}}" wx:for="{{n.children}}" wx:for-index="i" wx:for-item="tbody" wx:key="i">
                                                <view class="{{[tr.attrs.class]}}" style="{{(tr.attrs.style||'')+(tr.name[0]=='t'?';display:table-'+(tr.name=='tr'?'row':'cell'):'')}}" wx:for="{{tbody.children}}" wx:for-index="j" wx:for-item="tr" wx:key="j">
                                                    <trees bind:__l="__l" nodes="{{tr.children}}" vueId="{{'676b0423-3-'+index+'-'+i+'-'+j}}" wx:if="{{tr.name=='td'}}"></trees>
                                                    <block wx:else>
                                                        <trees bind:__l="__l" class="{{[td.attrs.class]}}" nodes="{{td.children}}" style="{{(td.attrs.style||'')+(td.name[0]=='t'?';display:table-'+(td.name=='tr'?'row':'cell'):'')}}" vueId="{{'676b0423-4-'+index+'-'+i+'-'+j+'-'+k}}" wx:for="{{tr.children}}" wx:for-index="k" wx:for-item="td" wx:key="k"></trees>
                                                    </block>
                                                </view>
                                            </view>
                                        </view>
                                        <block wx:else>
                                            <rich-text class="{{['_p __'+n.name]}}" id="{{n.attrs.id}}" nodes="{{[n]}}" wx:if="{{handler.useRichText(n)}}"></rich-text>
                                            <trees bind:__l="__l" class="{{[ (n.attrs.id||'')+' _'+n.name+' '+(n.attrs.class||'') ]}}" lazyLoad="{{lazyLoad}}" nodes="{{n.children}}" style="{{n.attrs.style}}" vueId="{{'676b0423-5-'+index}}" wx:else></trees>
                                        </block>
                                    </block>
                                </block>
                            </block>
                        </block>
                    </block>
                </block>
            </block>
        </block>
    </block>
</view>
<wxs module="handler" src="handler.wxs" />